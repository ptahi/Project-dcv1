<odoo>
    <record id="seq_sm_donhang" model="ir.sequence">
        <field name="name">Đơn hàng website</field>
        <field name="code">sm.donhang</field>
        <field name="prefix">DH</field>
        <field name="padding">5</field>
    </record>
    <record id="seq_sm_shopping_cart" model="ir.sequence">
